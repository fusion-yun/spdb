<?xml version="1.0" encoding="UTF-8"?>
<equilibrium>
    <ids_properties>
        <homogeneous_time dtype="int">0</homogeneous_time>
    </ids_properties>
    <time ><mdsplus>DATA(\TIME)</mdsplus></time>
    
    <time_slice id="*">
        <boundary>
            <type>1</type>
            <outline>
                <r ><mdsplus>DATA(\BDRY)[0, 0: DATA(\NBDRY)[{time_slice#id}]-1,{time_slice#id}]</mdsplus></r>
                <z ><mdsplus>DATA(\BDRY)[1, 0: DATA(\NBDRY)[{time_slice#id}]-1,{time_slice#id}]</mdsplus></z>
            </outline>
            <psi_norm dtype="NONE">??</psi_norm>
            <psi ><mdsplus>DATA(\SSIBRY)</mdsplus></psi>

            <geometric_axis>
                <r><mdsplus>DATA(\RSURF[*,{time_slice#id}])</mdsplus></r>
                <z><mdsplus>DATA(\ZSURF[*,{time_slice#id}])</mdsplus></z>
            </geometric_axis>

            <minor_radius dtype="NONE">??</minor_radius>

            <elongation><mdsplus>DATA(\KAPPA[{time_slice#id}])</mdsplus></elongation>
            <elongation_upper dtype="NONE">??</elongation_upper>
            <elongation_lower dtype="NONE">??</elongation_lower>

            <triangularity dtype="NONE">??</triangularity>
            <triangularity_lower><mdsplus>DATA(\TRIBOT[{time_slice#id}])</mdsplus></triangularity_lower>
            <triangularity_upper><mdsplus>DATA(\TRITOP[{time_slice#id}])</mdsplus></triangularity_upper>

            <!-- <x_point>
                <r dtype="NONE">??</r>
                <z dtype="NONE">??</z>
            </x_point>

            <strike_point>
                <r dtype="NONE">??</r>
                <z dtype="NONE">??</z>
            </strike_point>

            <active_limiter_point>
                <r dtype="NONE">??</r>
                <z dtype="NONE">??</z>
            </active_limiter_point> -->

        </boundary>

        <coordinate_system>
            <grid_type>
                <name>rectangular</name>
                <index>1</index>
                <description>Cylindrical R,Z ala eqdsk (R=dim1, Z=dim2). 
                    In this case the position arrays should not be filled since 
                    they are redundant with grid/dim1 and dim2.</description>
            </grid_type> 
            <grid>
                <dim1 ><mdsplus>DIM_OF(\R,0)</mdsplus></dim1>
                <dim2 ><mdsplus>DIM_OF(\R,1)</mdsplus></dim2>
            </grid>
            <r ><mdsplus>DATA(\R)</mdsplus></r>
            <z ><mdsplus>DATA(\Z)</mdsplus></z>
        </coordinate_system>                

        <global_quantities>
            <beta_pol ><mdsplus>DATA(\BETAP[{time_slice#id}])</mdsplus></beta_pol>
            <beta_tor ><mdsplus>DATA(\BETAT[{time_slice#id}])</mdsplus></beta_tor>
            <beta_normal ><mdsplus>DATA(\BETAN[{time_slice#id}])</mdsplus></beta_normal>

            <ip ><mdsplus>DATA(\EFIT_MFILE:PLASMA)[{time_slice#id}]</mdsplus></ip>

            <li_3 ><mdsplus>DATA(\LI)[{time_slice#id}]</mdsplus></li_3>

            <volume><mdsplus>DATA(\VOLUME)[{time_slice#id}]</mdsplus></volume>

            <!-- <area dtype="NONE">??</area> -->
            <!-- <surface dtype="NONE">??</surface> -->
            <!-- <length_pol dtype="NONE">??</length_pol> -->

        
            <psi_axis ><mdsplus>DATA(\SSIMAG)[{time_slice#id}]</mdsplus></psi_axis>

            <psi_boundary ><mdsplus>DATA(\SSIBRY)[{time_slice#id}]</mdsplus></psi_boundary>

            <magnetic_axis>
                <r ><mdsplus>DATA(\RMAXIS)[{time_slice#id}]</mdsplus></r>
                <z ><mdsplus>DATA(\ZMAXIS)[{time_slice#id}]</mdsplus></z>
                <b_field_tor ><mdsplus>DATA(\BCENTR)[{time_slice#id}]</mdsplus></b_field_tor>
            </magnetic_axis>

            <q_axis ><mdsplus>DATA(\Q0)[{time_slice#id}]</mdsplus></q_axis>
            <q_95 ><mdsplus>DATA(\Q95)[{time_slice#id}]</mdsplus></q_95>
            <q_min>
                <value ><mdsplus>DATA(\QL)[{time_slice#id}]</mdsplus></value>
                <rho_tor_norm  ><mdsplus>DATA(\AMINOR)[{time_slice#id}]</mdsplus></rho_tor_norm> <!-- need comment -->
            </q_min>

            <energy_mhd ><mdsplus>DATA(\WMHD)[{time_slice#id}]</mdsplus></energy_mhd>
        </global_quantities>

        <profiles_1d>
            <!-- <psi dtype="NONE">??</psi> -->
            <!-- <phi dtype="NONE">??</phi> -->
            <!-- <pressure dtype="NONE">??</pressure> -->

            <f ><mdsplus>DATA(\FPOL)[*,{time_slice#id}]</mdsplus></f>
            <dpressure_dpsi ><mdsplus>DATA(\PPRIME)[*,{time_slice#id}]</mdsplus></dpressure_dpsi>
            <f_df_dpsi ><mdsplus>DATA(\FFPRIM)[*,{time_slice#id}]</mdsplus></f_df_dpsi>

            <!-- <j_tor dtype="NONE">???</j_tor> -->
            <!-- <j_parallel dtype="NONE">???</j_parallel> -->

            <q ><mdsplus>DATA(\QPSI)[*,{time_slice#id}]</mdsplus></q>

            <!-- <geometric_axis>
                <r dtype="NONE" /> 
                <z dtype="NONE" /> 
            </geometric_axis> -->

            <!-- <magnetic_shear dtype="NONE" />  -->
            <!-- <r_inboard dtype="NONE" />  -->
            <!-- <r_outboard dtype="NONE" />  -->
            <!-- <rho_tor dtype="NONE" />  -->
            <!-- <rho_tor_norm dtype="NONE" />  -->
            <!-- <dpsi_drho_tor dtype="NONE" />  -->
            
            <!-- <elongation dtype="NONE" />  -->
            <!-- <triangularity_upper dtype="NONE" />  -->
            <!-- <triangularity_lower dtype="NONE" />  -->

            <volume ><mdsplus>DATA(\VOLUME)[*,{time_slice#id}]</mdsplus></volume>   
            <!-- <rho_volume_norm dtype="NONE" />  -->
            <!-- <dvolume_dpsi dtype="NONE" />  -->
            <!-- <dvolume_drho_tor dtype="NONE" />  -->

            <!-- <area dtype="NONE" />  -->
            
            <!-- <b_field_average dtype="NONE" />  -->
            <!-- <b_field_min dtype="NONE" />  -->
            <!-- <b_field_max dtype="NONE" />  -->
            <!-- <beta_pol dtype="NONE" />  -->
            <!-- <mass_density dtype="NONE" />  -->


        </profiles_1d>

        <profiles_2d>   
            <!-- <r dtype="NONE"/> -->

            <!-- <z dtype="NONE"/> -->

            <psi ><mdsplus tree="EAST_EFIT">DATA(\PSIRZ)[*,*,{time_slice#id}]</mdsplus></psi>

            <grid>
                <dim1 ><mdsplus>DATA(\R)[*]</mdsplus></dim1>
                <dim2 ><mdsplus>DATA(\Z)[*]</mdsplus></dim2>
            </grid>

            <!-- <theta dtype="NONE"/> -->

            <!-- <phi dtype="NONE"/> -->
            
            <!-- <j_tor dtype="NONE"/> -->

            <!-- <j_parallel dtype="NONE"/> -->

            <!-- <b_field_r dtype="NONE"/> -->

            <!-- <b_field_z dtype="NONE"/> -->

            <!-- <b_field_tor dtype="NONE"/> -->

        </profiles_2d>  

    </time_slice>
</equilibrium>
